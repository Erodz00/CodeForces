import java.util.Scanner;

public class Codeforces1288B {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		for (int i = 0; i < n; i++) {
			long a = scan.nextInt();
			long b = scan.nextInt();
			if (b == String.valueOf(Math.abs(b + 1)).length() - 1)
				System.out.println((String.valueOf(Math.abs(b + 1)).length()) * a);
			else
				System.out.println((String.valueOf(Math.abs(b + 1)).length() - 1) * a);
		}
		scan.close();
	}
}
